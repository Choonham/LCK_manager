<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <TextView
            android:theme="@style/Theme.LCK_manager.commonTextTheme"
            android:text="1"
            android:textStyle="bold"
            android:textSize="18sp"
            android:layout_width="0dp"
            android:layout_height="40dp" android:id="@+id/team_rank_league_ranking" android:layout_weight="0.1"/>
    <TextView
            android:theme="@style/Theme.LCK_manager.commonTextTheme"
            android:text="T1"
            android:layout_width="0dp"
            android:layout_height="40dp" android:id="@+id/team_name_league_ranking" android:layout_weight="0.4"/>
    <TextView
            android:theme="@style/Theme.LCK_manager.commonTextTheme"
            android:text="4"
            android:layout_width="0dp"
            android:layout_height="40dp" android:id="@+id/team_win_league_ranking" android:layout_weight="0.1"/>
    <TextView
            android:theme="@style/Theme.LCK_manager.commonTextTheme"
            android:text="0"
            android:layout_width="0dp"
            android:layout_height="40dp" android:id="@+id/team_lose_league_ranking" android:layout_weight="0.1"/>
    <TextView
            android:theme="@style/Theme.LCK_manager.commonTextTheme"
            android:text="1.0"
            android:layout_width="0dp"
            android:layout_height="40dp" android:id="@+id/team_win_ratio_league_ranking" android:layout_weight="0.15"/>
    <TextView
            android:theme="@style/Theme.LCK_manager.commonTextTheme"
            android:text="+8"
            android:layout_width="0dp"
            android:layout_height="40dp" android:id="@+id/team_win_diff_league_ranking" android:layout_weight="0.15"/>

</LinearLayout>